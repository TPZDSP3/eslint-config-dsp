{
  "name": "@tpzdsp/eslint-config-dsp",
  "version": "2.0.0",
  "description": "re-usable config for ESLint",
  "main": "index.js",
  "engines": {
    "npm": ">=10.5.0",
    "node": ">=20.12.2"
  },
  "packageManager": "yarn@1.22.19",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpzdsp3/eslint-config-dsp.git"
  },
  "keywords": [
    "eslint",
    "config",
    "eslintconfig"
  ],
  "author": "Mark Small <marksmall@gmx.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpzdsp3/eslint-config-dsp/issues"
  },
  "homepage": "https://github.com/tpzdsp3/eslint-config-dsp#readme",
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint:rules": "eslint --print-config rules > eslint-rules.json",
    "release": "yarn clean && yarn && semantic-release"
  },
  "peerDependencies": {
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "semantic-release": "^22.0.0"
  },
  "devDependencies": {
    "@mark-small/semantic-release-config-marksmall": "^1.0.1",
    "@stylistic/eslint-plugin": "^3.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "semantic-release": "^22.0.0"
  },
  "release": {
    "extends": "./release.config.js"
  }
}